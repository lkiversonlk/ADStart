extends ../layout

block content
   section.header
       div.navbar.navbar-default
           div.container(style="height:3em;text-align:center")
               img(src="/images/title.png", height="100%")

   section.container-fluid.user_info
       div.row
           div(style="width:50px; float:left; padding-left:10%")
               img(src=user.iconUrl, style="width:100%")
           div(style="float:left; padding-left:10%; height: 50px")
                   | #{user.name}

   section.container.delegations


